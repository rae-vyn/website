---
import '../styles/style.css';
let { title } = Astro.props;

title = title ? title : "Raevyn's Site";
---
<meta charset="utf-8" />
<link rel="icon" href="/favicon.ico" />
<meta name="viewport" content="width=device-width" />
<meta name="generator" content={Astro.generator} />
<meta content={title} property="og:title" />
<meta content="Raevyn's very own corner of the web." property="og:description" />
<meta content="https://raevyn.art" property="og:url" />
<meta content="https://raevyn.art/images/eldrae_raedient.png" property="og:image" />
<meta content="#caaeed" data-react-helmet="true" name="theme-color" />
